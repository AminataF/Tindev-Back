{% extends 'base.html.twig' %}

{% block title %}Project{% endblock %}

{% block body %}
    <h1>Project</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ project.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ project.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ project.description }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ project.date ? project.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Techno</th>
                <td>{{ project.techno }}</td>
            </tr>
            <tr>
                <th>Github</th>
                <td>{{ project.github }}</td>
            </tr>
            <tr>
                <th>UrlProject</th>
                <td>{{ project.urlProject }}</td>
            </tr>
            <tr>
                <th>Upload</th>
                <td>{{ project.upload }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ project.createdAt ? project.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdateAt</th>
                <td>{{ project.UpdateAt ? project.UpdateAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_backoffice_project_index') }}">back to list</a>

    <a href="{{ path('app_backoffice_project_edit', {'id': project.id}) }}">edit</a>

    {{ include('backoffice/project/_delete_form.html.twig') }}
{% endblock %}
